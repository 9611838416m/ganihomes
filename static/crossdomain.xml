<cross-domain-policy xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.adobe.com/xml/schemas/PolicyFile.xsd">
<allow-access-from domain="www.ganihomes.com"/>
<allow-access-from domain="ganihomes.com"/>
<allow-access-from domain="static.ganihomes.com"/>
<allow-access-from domain="static.gh"/>
<allow-access-from domain="gh"/>
<site-control permitted-cross-domain-policies="master-only"/>
<allow-http-request-headers-from domain="*.ganihomes.com" headers="*" secure="true"/>
</cross-domain-policy>
